{
  "schemaVersion": 1,
  "id": "jigsawlogiclib",
  "version": "${version}",

  "name": "Jigsaw Logic Lib",
  "description": "Allows for custom game logic to occur when a jigsaw structure is generated and loaded.",
  "authors": [
    "Totobird Creations"
  ],
  "contact": {
    "homepage": "https://github.com/Totobird-Creations",
    "sources": "https://github.com/Totobird-Creations/JigsawLogicLib"
  },

  "license": "MIT",
  "icon": "assets/jigsawlogiclib/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.totobirdcreations.jigsawlogiclib.init.Main"
    ],
    "client": [
      "net.totobirdcreations.jigsawlogiclib.init.Client"
    ]
  },
  "mixins": [
    "mixins.jigsawlogiclib.json"
  ],
  "accessWidener": "jigsawlogiclib.accesswidener",

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
  }
}
